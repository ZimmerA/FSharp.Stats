			<li><a href="@Root/Rank.html">Rank</a></li>
			<li><a href="@Root/Quantile.html">Quantile</a></li> 
			<li class="divider"></li>
			<li><a href="@Root/Matrix_Vector.html">Matrix and Vector</a></li>
			@if (Properties.Dictionary.ContainsKey("page-source") && Properties["page-source"].ToString().EndsWith("Matrix_Vector.fsx"))
              {
				  <ul class="nav nav-list">
				  	<li><a href="@Root/Matrix_Vector.html#Vector">Vector</a></li>
					<li><a href="@Root/Matrix_Vector.html#Matrix">Matrix</a></li>
				  </ul>
              }
			  </li>
			<li class="divider"></li>
			<li><a href="@Root/SpecialFunctions.html">Special Functions</a></li> 
			@if (Properties.Dictionary.ContainsKey("page-source") && Properties["page-source"].ToString().EndsWith("SpecialFunctions.fsx"))

					<li><a href="@Root/Testing.html#PvalueAdjust">Adjusting P-Values</a></li>
				  </ul>
              }
			</li>
			<li><a href="@Root/Fitting.html">Fitting</a></li>
			@if (Properties.Dictionary.ContainsKey("page-source") && Properties["page-source"].ToString().EndsWith("Fitting.fsx"))
              {
